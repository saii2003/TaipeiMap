@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>


    <meta name="viewport" content="initial-scale=1.0">
    <title>googlemap</title>
    <style type="text/css"> 
	  html { height: 100%; } 
	  body { height: 100%; margin: 0px;} 
	  #map { height: 70% ; width:70%;} 
	 
	</style> 

</head>
<body>
   <div id="map"></div>
    <script>

        var map;
        function initMap() {
            map = new google.maps.Map(document.getElementById('map'), {
                center: { lat: -34.397, lng: 150.644 },
                zoom: 8
            });
        }

    </script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCbBZ2YfJVXGDif2Fv9ggvr7V8JepMX5xw&callback=initMap"
        async defer></script>
 
</body>
</html>
